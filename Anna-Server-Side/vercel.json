{
    "version":2,
    "builds": [
      { "src": "*.js", "use": "@vercel/node" }
    ],
  "routes": [
    {
      "src": "contact(.*)",
      "dest": "./index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
      
  }